{
  "name": "taskolio-vscode-client",
  "displayName": "taskolio-vscode-client",
  "description": "Expose vscode editors to taskolio bookmarks for use with MIDI/DJ controllers.",
  "version": "0.0.1",
  "publisher": "visophyte",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "taskolio.pushBookmark",
        "title": "Push Bookmark"
      }
    ],
    "menus": {
      "explorer/context": [{
        "command": "taskolio.pushBookmark",
        "group": "navigation"
      }]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "ws": "^7.4.6"
  }
}